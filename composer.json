{
    "name" : "ekyna/core-bundle",
    "description" : "Various utilities",
    "type" : "symfony-bundle",
    "authors" : [{
            "name" : "Etienne Dauvergne",
            "homepage" : "http://ekyna.com"
        }
    ],
    "keywords" : [
        "symfony2",
        "bundle"
    ],
    "homepage" : "https://github.com/ekyna/CoreBundle",
    "license" : [
        "MIT"
    ],
    "require" : {
        "php" : ">=5.4.0",
        "symfony/framework-bundle" : "~2.3",
        "symfony-cmf/routing" : "1.2.*@dev",
        "twig/extensions": "~1.0",
        "doctrine/doctrine-bundle" : "~1.2",
        "doctrine/doctrine-cache-bundle":  "~1.0",
        "doctrine/doctrine-migrations-bundle": "dev-master",
        "friendsofsymfony/jsrouting-bundle": "@stable",
        "friendsofsymfony/elastica-bundle": "3.0.*",
        "jms/serializer-bundle": "0.13.*@dev",
        "stof/doctrine-extensions-bundle" : "~1.1@dev",
        "knplabs/knp-menu-bundle" : "~2.0.@dev",
        "knplabs/knp-gaufrette-bundle" : "0.2.*@dev",
        "liip/imagine-bundle": "0.*@dev",
        "malsup/form": "dev-master",
        "twbs/bootstrap" : "3.1.*",
        "braincrafted/bootstrap-bundle" : "2.0.0",
        "stfalcon/tinymce-bundle" : "0.3.*@dev"
    },
    "autoload" : {
        "psr-0" : {
            "Ekyna\\Bundle\\CoreBundle" : ""
        }
    },
    "target-dir" : "Ekyna/Bundle/CoreBundle",
    "extra" : {
        "branch-alias" : {
            "dev-master" : "0.1.x-dev"
        }
    }
}